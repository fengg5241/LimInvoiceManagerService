server.port=8080
server.servlet.contextPath=/
debug=true
logging.level.root=warn
logging.level.tk.mybatis.springboot.mapper=trace
spring.datasource.url=jdbc:mysql://10.0.75.1:3306/gion_dining_cms?useUnicode=true&characterEncoding=utf8&allowMultiQueries=true&serverTimezone=GMT%2B8
spring.datasource.username=root
spring.datasource.password=root
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.druid.initial-size=1
spring.datasource.druid.min-idle=1
spring.datasource.druid.max-active=20
spring.datasource.druid.test-on-borrow=true
spring.datasource.druid.stat-view-servlet.allow=true

# mybatis \u914D\u7F6E
mybatis.type-aliases-package=com.gion.cms.entity
mybatis.mapper-locations=classpath:sqlmapper/*.xml
# \u901A\u7528 Mapper \u914D\u7F6E
mapper.mappers=com.gion.cms.mapper
mapper.not-empty=false
mapper.identity=MYSQL